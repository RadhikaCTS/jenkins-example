pipeline {
    agent any

    stages {
        stage ('Validation Stage') {

            steps {
                withMaven(maven : 'LocalMaven') {
                    sh 'mvn clean validate'
                }
            }
        }
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'LocalMaven') {
                    sh 'mvn clean compile'
            }
        }
     }

    }
}
